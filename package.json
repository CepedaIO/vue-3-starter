{
  "name": "vue-3-starter",
  "version": "0.0.0",
  "scripts": {
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite --host",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "deploy": "yarn build && gcloud app deploy"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.16.0",
    "postcss": "^8.3.6",
    "postcss-pxtorem": "^6.0.0",
    "precss": "^4.0.0",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2"
  }
}
