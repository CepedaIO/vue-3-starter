{
  "name": "vue-3-starter",
  "version": "0.0.0",
  "scripts": {
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite --host",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "deploy": "yarn build && gcloud app deploy"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.3.1",
    "eslint": "^8.8.0",
    "eslint-plugin-vue": "^8.4.1",
    "postcss": "^8.3.6",
    "postcss-pxtorem": "^6.0.0",
    "precss": "^4.0.0",
    "sass": "~1.49.7",
    "sass-loader": "^12.4.0",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-tsc": "^0.31.2"
  }
}
